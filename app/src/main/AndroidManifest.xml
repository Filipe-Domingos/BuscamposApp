<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="br.edu.iff.pooa20152.buscampos">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/logo"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".activity.LoginTela">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
            <activity android:name=".activity.PrincipalTela" />
            <activity android:name=".activity.CadastroUsuarioTela" />
            <activity android:name=".activity.CadastroPontoTela" />
            <activity android:name=".activity.CadastroOnibusTela" />
            <activity android:name=".activity.CadastroLinhaTela" />
            <activity android:name=".activity.CadastroHorarioTela" />
            <activity android:name=".activity.CadastroEmpresaTela">
        </activity>
    </application>

</manifest>